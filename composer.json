{
    "name": "dmk/t3rest",
    "description": "Provides a REST interface for TYPO3.",
    "version": "9.0.0",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "rest",
        "router"
    ],
    "homepage": "http://www.dmk-ebusiness.de/",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "RenÃ© Nitzsche",
            "email": "nitzsche@dmk-ebusiness.de",
            "role": "Developer"
        },
        {
            "name": "Michael Wagner",
            "email": "michael.wagner@dmk-ebusiness.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dev@dmk-ebusiness.de"
    },
    "require": {
        "typo3/cms-core": "^7.6 || ^8.7 || ^9.5",
        "digedag/rn-base" : ">=1.7.1"
    },
    "replace": {
        "typo3-ter/t3rest": "*"
    },
    "require-dev": {
        "nimut/testing-framework": "*"
    },
    "autoload": {
        "psr-4": {
            "DMK\\T3rest\\": "Classes"
        },
        "classmap": [
            "cache/",
            "Classes/",
            "exception/",
            "controller/",
            "decorator/",
            "exception/",
            "mod/",
            "models/",
            "provider/",
            "search/",
            "srv/",
            "util/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DMK\\T3rest\\Tests\\": "Tests"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "t3rest"
        }
    }
}
